<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="selectDialog"
		title="Products"
		items="{/items}"
		search=".onValueHelpSearch"
		confirm=".onValueHelpClose"
		cancel=".onValueHelpClose"
		multiSelect="true">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{nombre}"
			description="{nombre}" />
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{categoria}"
			description="{categoria}" />
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{estado}"
			description="{estado}" />
		</SelectDialog>
</core:FragmentDefinition>