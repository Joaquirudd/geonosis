<mvc:View controllerName="geonosis.table.report.controller.Main"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">

    <Page id="page" title="{i18n>title}">
        <IconTabBar
		id="idIconTabBarNoIcons"
		expanded="{device>/isNoPhone}"
		class="sapUiResponsiveContentPadding">
		<items>
			<IconTabFilter text="Tabla y Filtros" key="info">
				<core:Fragment fragmentName="geonosis.table.report.view.fragments.Table" type="XML"/>
			</IconTabFilter>
			<IconTabFilter text="Formulario" key="attachments">
				<Text text="Attachments go here ..." />
			</IconTabFilter>
		</items>
	</IconTabBar>
    </Page>
</mvc:View>